{
    "name": "brahmic/laravel-filler",
    "description": "Laravel Eloquent Database Filler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "brahmic",
            "email": "brhamable@yandex.net"
        },
        {
            "name": "greab–æck",
            "email": "greabock@example.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "ramsey/uuid": "^4.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Brahmic\\Filler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brahmic\\Filler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brahmic\\Filler\\FillerServiceProvider"
            ]
        }
    }
}
